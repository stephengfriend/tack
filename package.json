{
  "name": "tack",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "modules/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "prettier": {
		"semi": false,
		"trailingComma": "all",
		"singleQuote": true,
		"useTabs": true
	},
  "devDependencies": {
    "eslint-config-custom": "^0.0.0",
    "prettier": "^2.7.1",
    "turbo": "^1.4.0"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=14.16.0"
  },
  "packageManager": "npm@8.15.0",
  "type": "module"
}
